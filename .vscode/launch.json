{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "dev",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeVersion": "16",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["dev"],
      "console": "integratedTerminal"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Redux Debugger",
      "url": "http://localhost:5010"
    },
    {
      "name": "prune",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeVersion": "16",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["game", "prune-imports:watch"],
      "console": "integratedTerminal"
    },
    {
      "name": "lint",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeVersion": "16",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["lint"],
      "console": "integratedTerminal"
    },
    {
      "name": "redux-devtools",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["game", "redux-devtools"],
      "console": "integratedTerminal"
    }
  ]
}
